{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "microsoft-edge/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "webview2",
        "src": "webview2",
        "dest": "webview2",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "microsoft-edge/**",
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.png",
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "group": "webview2",
        "src": "webview2",
        "dest": "webview2"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/microsoft-edge/breadcrumbs/toc.json",
      "titleSuffix": "Microsoft Edge Development",
      "uhfHeaderId": "MSDocsHeader-MSEdge",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/edge-developer",
      "feedback_product_url": "https://microsoftedge.uservoice.com",
      "extendBreadcrumb": true
    },
    "fileMetadata": {
      "ms.technology":{
        "dev-guide/**.md": "edgehtml",
        "devtools-guide/**.md": "devtools",
        "devtools-guide-chromium/**.md": "devtools",
        "devtools-protocol/**.md": "devtools",
        "extensions/**.md": "extensions",
        "extensions-chromium/**.md": "extensions",
        "hosting/chakra-hosting/**.md": "chakra",
        "hosting/webview/**.md": "webview",
        "progressive-web-apps-edgehtml/**.md": "pwa",
        "progressive-web-apps-chromium/**.md": "pwa",
        "webview2/**.md": "webview2",
        "windows-runtime/**.md": "windows-integration"
      }
    },
    "template": [
      "docs.html"
    ],
    "dest": "microsoft-edge",
    "groups": {
      "webview2": {
        "dest": "webview2",
        "moniker_range": ">= webview2-0.9.430 <= webview2-0.9.538-prerelease"
      }
    },
    "markdownEngineName": "markdig"
  }
}
